{
  "name": "diecast-tracker",
  "version": "1.0.0",
  "description": "Hot Wheels Die-Cast Collection Management System with CLI and Web Interface",
  "main": "app.py",
  "scripts": {
    "start": "python start_web.py",
    "start-with-venv": "activate_venv.bat && python start_web.py",
    "web": "python start_web.py",
    "dev": "python app.py",
    "cli": "python main.py",
    "add": "python pages/add-model/add_model.py",
    "update": "python scripts/update_model.py",
    "delete": "python scripts/delete_model.py",
    "search": "python scripts/search_model.py",
    "stats": "python scripts/statistics.py",
    "field": "python pages/add-field/add_field.py",
    "convert-year": "python scripts/convert_year_format.py",
    "excel": "start data\\HW_list.xlsx",
    "install": "pip install -r requirements.txt",
    "freeze": "pip freeze > requirements.txt",
    "activate-venv": "activate_venv.bat",
    "serve": "python start_web.py",
    "help": "echo Available commands: start(web), web, dev, cli, add, search, stats, field, excel, install, freeze, venv, activate, activate-venv, activate-ps",
    "test": "python -c \"print('DieCastTracker is working!')\""
  },
  "keywords": [
    "hot-wheels",
    "die-cast",
